{
  "name": "chatmoss-ui",
  "version": "5.7.22",
  "private": false,
  "description": "chatmoss-ui",
  "author": "AICCOF",
  "keywords": [
    "chatmoss-ui",
    "chatgpt"
  ],
  "scripts": {
    "updater": "node scripts/updater.mjs",
    "release": "node scripts/release.mjs",
    "dev": "vite",
    "build": "run-p build-vscode-only",
    "mac": "run-p type-check build-mac-only && npm run tauri-build",
    "mac-dev": "run-p build-mac-only && npm run tauri-debug",
    "win": "run-p type-check build-win-only && npm run tauri-build",
    "win-dev": "run-p type-check build-win-only && npm run tauri-debug",
    "vscode": "run-p type-check build-vscode-only && npm run tauri-build",
    "vscode-dev": "run-p type-check build-vscode-only && npm run tauri-debug",
    "preview": "vite preview",
    "build-test": "vite build",
    "build-mac-only": "node index.js",
    "build-win-only": "node index.js",
    "build-vscode-only": "vite build --mode vscode",
    "build-cf-only": "vite build --mode cf",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "bootstrap": "pnpm install && pnpm run common:prepare",
    "common:cleanup": "rimraf node_modules && rimraf pnpm-lock.yaml",
    "common:prepare": "husky install",
    "tauri": "tauri",
    "tauri-debug": "yarn tauri build --debug",
    "tauri-build": "yarn tauri build"
  },
  "dependencies": {
   
  },
  "devDependencies": {
    "@actions/github": "^5.1.1",
    "@antfu/eslint-config": "^0.35.3",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@iconify/vue": "^4.1.0",
    "@tauri-apps/cli": "^1.2.3",
    "@types/crypto-js": "^4.1.1",
    "@types/katex": "^0.16.0",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.14.6",
    "@vitejs/plugin-vue": "^4.0.0",
    "autoprefixer": "^10.4.13",
    "axios": "^1.3.4",
    "crypto-js": "^4.1.1",
    "eslint": "^8.35.0",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "lint-staged": "^13.1.2",
    "node-fetch": "^3.3.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "rimraf": "^4.2.0",
    "tailwindcss": "^3.2.7",
    "typescript": "~4.9.5",
    "vite": "^4.1.4",
    "vue-tsc": "^1.2.0"
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": [
      "pnpm lint:fix"
    ]
  }
}
